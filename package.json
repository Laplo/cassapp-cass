{
  "name": "cassbar",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.1.2",
    "@bit/mui-org.material-ui.linear-progress": "^4.9.10",
    "@react-keycloak/web": "2.1.3",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "autoprefixer": "9.8.6",
    "dotenv": "8.2.0",
    "graphql": "15.3.0",
    "keycloak-js": "11.0.0",
    "postcss-cli": "7.1.1",
    "qrcode.react": "1.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-scroll-trigger": "0.6.6",
    "react-toastify": "^6.0.8",
    "react-web-notifications": "1.1.0",
    "subscriptions-transport-ws": "0.9.17",
    "tailwind": "4.0.0",
    "tailwindcss": "1.6.2",
    "uuid": "8.3.0",
    "zustand": "2.2.4"
  },
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "npm config set @bit:registry && npm run build:css && react-scripts build",
    "build:serve": "npm run build:css && react-scripts build && serve -s build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/main.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
